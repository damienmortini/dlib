{
  "name": "@damienmortini/elements",
  "version": "0.2.8",
  "description": "Minimalist and reusable custom elements",
  "main": "src/index.js",
  "scripts": {
    "start": "server --path=?dev",
    "docs": "jsdoc -c jsdoc.config.json",
    "bump-patch": "npm version patch && npm publish --access=public",
    "bump-minor": "npm version minor && npm publish --access=public",
    "bump-major": "npm version major && npm publish --access=public",
    "fix": "eslint src --fix",
    "test": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/damienmortini/elements.git"
  },
  "keywords": [
    "custom elements",
    "web components"
  ],
  "author": "Damien Mortini",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/damienmortini/elements/issues"
  },
  "homepage": "https://github.com/damienmortini/elements#readme",
  "dependencies": {
    "@damienmortini/lib": "^0.2.11",
    "@simonwep/pickr": "^1.5.0"
  },
  "devDependencies": {
    "@damienmortini/server": "^1.0.12",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "jsdoc": "^3.6.3"
  }
}
